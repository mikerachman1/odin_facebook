<%= form_with model: [ @user, @user.build_profile ] do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :age %>
  <%= form.text_field :age %>

  <%= form.label :location %>
  <%= form.text_field :location %>

  <%= form.label :picture %>
  <%= form.text_field :picture %>

  <%= form.submit %>
<% end %>